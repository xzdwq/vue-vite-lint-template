{
  "name": "vue-xzdwq-template",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "prod": "vue-tsc --noEmit && vite build && vite preview",
    "lint-check": "eslint \"./**/*.{js,ts,vue}\"",
    "lint-fix": "eslint \"./**/*.{js,ts,vue}\" --fix",
    "prettier-check": "npx prettier --check .",
    "prettier-fix": "npx prettier --write .",
    "stylelint-check": "npx stylelint \"./**/*.{css,scss,vue}\"",
    "stylelint-fix": "npx stylelint \"**/*.{css,scss,vue}\" --fix",
    "prepare": "simple-git-hooks"
  },
  "dependencies": {
    "@quasar/extras": "^1.15.5",
    "@vueuse/core": "^9.4.0",
    "quasar": "^2.10.0",
    "vue": "^3.2.41",
    "vue-error-boundary": "^2.0.1",
    "vue-i18n": "^9.2.2",
    "vue-router": "4.1.6"
  },
  "devDependencies": {
    "@babel/types": "^7.18.10",
    "@eslint/eslintrc": "^1.3.0",
    "@quasar/vite-plugin": "^1.2.1",
    "@rollup/plugin-yaml": "^4.0.1",
    "@twind/typescript-plugin": "^0.3.5",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "@typescript-eslint/utils": "^5.32.0",
    "@vitejs/plugin-vue": "^3.0.0",
    "@vue/compiler-sfc": "3.2.37",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "^22.0.0",
    "eslint-formatter-pretty": "^4.1.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-html": "^7.1.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsdoc": "^39.3.6",
    "eslint-plugin-jsonc": "^2.3.1",
    "eslint-plugin-markdown": "^3.0.0",
    "eslint-plugin-n": "^15.2.4",
    "eslint-plugin-no-use-extend-native": "^0.5.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-unicorn": "^43.0.2",
    "eslint-plugin-vue": "^9.3.0",
    "eslint-plugin-yml": "^1.1.0",
    "jsonc-eslint-parser": "^2.1.0",
    "lint-staged": "13.0.3",
    "prettier": "2.7.1",
    "sass": "~1.26.11",
    "simple-git-hooks": "^2.8.0",
    "stylelint": "^14.9.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-prettier-scss": "^0.0.1",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard": "^26.0.0",
    "stylelint-config-standard-scss": "^5.0.0",
    "stylelint-scss": "^4.3.0",
    "twind": "^0.16.17",
    "typescript": "^4.6.4",
    "unplugin-auto-import": "^0.11.2",
    "unplugin-vue-components": "^0.22.7",
    "vite": "^3.2.1",
    "vite-plugin-eslint": "^1.7.0",
    "vitest": "^0.24.3",
    "vue-eslint-parser": "^9.0.3",
    "vue-tsc": "^1.0.9",
    "yaml-eslint-parser": "^1.1.0"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,vue}": [
      "eslint --fix"
    ],
    "*.{md,yaml,yml}": [
      "prettier --write"
    ],
    "*.{html,vue,css,scss}": [
      "stylelint --fix"
    ]
  }
}
